(state
  (init
    (starter1 (cond (with-backslash-prefix)
		    (1 (delete @-1) (pushback 1) (shift main1))))
    (starter2 (cond (with-backslash-prefix (shift main2))
		    (1 (commit)))))

  (main1
    (math (shift back))
    (filler (shift back))
    (backspace (undo)))

  (main2
    (starter2 (shift init))
    (math (move 1) (delete @-1) (move @>) (shift back))
    (filler (shift back))
    (backspace (undo)))

  (back
   (backspace (undo))))
