 (medial
  ("u" ?u)
  ("j" ?j)
  ("m" ?m)
 )
 (medial-map
  ("u" ?ㄧ)
  ("j" ?ㄨ)
  ("m" ?ㄩ)
 )
 (final
  ("8" ?8)
  ("i" ?i)
  ("k" ?k)
  ("," ?,)
  ("9" ?9)
  ("o" ?o)
  ("l" ?l)
  ("." ?.)
  ("0" ?0)
  ("p" ?p)
  (";" ?;)
  ("/" ?/)
  ("-" ?-)
 )
 (final-map
  ("8" ?ㄚ)
  ("i" ?ㄛ)
  ("k" ?ㄜ)
  ("," ?ㄝ)
  ("9" ?ㄞ)
  ("o" ?ㄟ)
  ("l" ?ㄠ)
  ("." ?ㄡ)
  ("0" ?ㄢ)
  ("p" ?ㄣ)
  (";" ?ㄤ)
  ("/" ?ㄥ)
  ("-" ?ㄦ)
 )

 (tone
  ("\ " ?\ )
  ("3" ?3)
  ("4" ?4)
  ("6" ?6)
  ("7" ?7)
 )
 (tone-map
  ("\ " ?\ )
  ("3" ?ˇ)
  ("4" ?ˋ)
  ("6" ?ˊ)
  ("7" ?˙)
 )
 (symbol-map
  ("[" ?「)
  ("]" ?」)
  ("{" ?『)
  ("}" ?』)
  ("'" ?、)
  ("<" ?，)
  (">" ?。)
 )

